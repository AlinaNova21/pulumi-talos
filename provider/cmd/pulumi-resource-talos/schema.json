{
    "name": "talos",
    "description": "A Pulumi package for creating and managing talos config",
    "keywords": [
        "talos",
        "kubernetes",
        "pulumi"
    ],
    "homepage": "https://talos.dev",
    "license": "Apache-2.0",
    "repository": "https://github.com/frezbo/pulumi-provider-talos",
    "config": {},
    "types": {
        "talos:bundle:Certs": {
            "description": "Talos Certs type",
            "properties": {
                "Admin": {
                    "$ref": "#/types/talos:bundle:PEMEncodedCertificateAndKey"
                },
                "Etcd": {
                    "$ref": "#/types/talos:bundle:PEMEncodedCertificateAndKey"
                },
                "K8s": {
                    "$ref": "#/types/talos:bundle:PEMEncodedCertificateAndKey"
                },
                "K8sAggregator": {
                    "$ref": "#/types/talos:bundle:PEMEncodedCertificateAndKey"
                },
                "K8sServiceAccount": {
                    "$ref": "#/types/talos:bundle:PEMEncodedKey"
                },
                "OS": {
                    "$ref": "#/types/talos:bundle:PEMEncodedCertificateAndKey"
                }
            },
            "type": "object"
        },
        "talos:bundle:Cluster": {
            "description": "Talos Cluster type",
            "properties": {
                "Id": {
                    "type": "string"
                },
                "Secret": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "talos:bundle:PEMEncodedCertificateAndKey": {
            "description": "Talos PEMEncodedCertificateAndKey type",
            "properties": {
                "Crt": {
                    "$ref": "pulumi.json#/Any"
                },
                "Key": {
                    "$ref": "pulumi.json#/Any"
                }
            },
            "type": "object"
        },
        "talos:bundle:PEMEncodedKey": {
            "description": "Talos PEMEncodedKey type",
            "properties": {
                "Key": {
                    "$ref": "pulumi.json#/Any"
                }
            },
            "type": "object"
        },
        "talos:bundle:Secrets": {
            "description": "Talos Secrets type",
            "properties": {
                "AESCBCEncryptionSecret": {
                    "type": "string"
                },
                "BootstrapToken": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "talos:bundle:SecretsBundle": {
            "description": "Talos SecretsBundle type",
            "properties": {
                "Certs": {
                    "$ref": "#/types/talos:bundle:Certs"
                },
                "Cluster": {
                    "$ref": "#/types/talos:bundle:Cluster"
                },
                "Secrets": {
                    "$ref": "#/types/talos:bundle:Secrets"
                },
                "TrustdInfo": {
                    "$ref": "#/types/talos:bundle:TrustdInfo"
                }
            },
            "type": "object"
        },
        "talos:bundle:TalosMachineConfigVersion": {
            "type": "string",
            "enum": [
                {
                    "description": "Talos ",
                    "value": "v1alpha1"
                }
            ]
        },
        "talos:bundle:TrustdInfo": {
            "description": "Talos TrustdInfo type",
            "properties": {
                "Token": {
                    "type": "string"
                }
            },
            "type": "object"
        }
    },
    "provider": {
        "description": "The provider type for the Talos package",
        "type": "object"
    },
    "resources": {
        "talos:bundle:secretsBundle": {
            "description": "Talos secretsBundle resource",
            "properties": {
                "secretsBundle": {
                    "type": "object",
                    "$ref": "#types/talos:bundle:SecretsBundle"
                }
            },
            "type": "object",
            "required": [
                "secretsBundle"
            ],
            "inputProperties": {
                "configVersion": {
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "string",
                            "$ref": "#types/talos:bundle:TalosMachineConfigVersion"
                        }
                    ],
                    "description": "the desired machine config version to generate (default \"v1alpha1\")",
                    "default": "v1alpha1"
                },
                "talosVersion": {
                    "type": "string",
                    "description": "the desired Talos version to generate config for (backwards compatibility, e.g. v0.8)"
                }
            }
        }
    },
    "language": {
        "go": {
            "generateResourceContainerTypes": true,
            "importBasePath": "github.com/frezbo/pulumi-provider-talos/sdk/v3/go/talos",
            "packageImportAliases": {
                "github.com/frezbo/pulumi-provider-talos/sdk/v3/go/talos/bundle": "bundle"
            }
        },
        "nodejs": {
            "dependencies": {
                "@pulumi/pulumi": "^3.0.0"
            },
            "devDependencies": {
                "@types/node": "^17.0.0",
                "typescript": "^3.7.0"
            }
        }
    }
}